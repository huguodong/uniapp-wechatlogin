.mp-login {
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.cu-btn.cuIcon {
  width: 84rpx;
  height: 84rpx;
}
.avatar {
  width: 90rpx;
  height: 90rpx;
}
page {
  background: #fff;
}
.container {
  padding-top: 115px;
  position: relative;
  width: 100vw;
  height: 100vh;
  overflow: hidden;
  background: #fff;
}
.left-top-sign {
  font-size: 120rpx;
  color: #f8f8f8;
  position: relative;
  left: -16rpx;
}
.right-top-sign {
  position: absolute;
  top: 80rpx;
  right: -30rpx;
  z-index: 95;
}
.right-top-sign:before, .right-top-sign:after {
  display: block;
  content: "";
  width: 400rpx;
  height: 80rpx;
  background: #b4f3e2;
}
.right-top-sign:before {
  -webkit-transform: rotate(50deg);
          transform: rotate(50deg);
  border-radius: 0 50px 0 0;
}
.right-top-sign:after {
  position: absolute;
  right: -198rpx;
  top: 0;
  -webkit-transform: rotate(-50deg);
          transform: rotate(-50deg);
  border-radius: 50px 0 0 0;
  /* background: pink; */
}
.left-bottom-sign {
  position: absolute;
  left: -270rpx;
  bottom: -320rpx;
  border: 100rpx solid #d0d1fd;
  border-radius: 50%;
  padding: 180rpx;
}
.login {
  border-color: #007AFF;
  margin-top: 156rpx;
  margin-right: 72rpx;
  margin-left: 72rpx;
}
.login-title {
  font-size: 56rpx;
  font-weight: 500;
}
.login-explain {
  font-size: 28rpx;
  color: #9E9E9E;
}
.login-phone {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-bottom: 2rpx #dedede solid;
  margin-top: 56rpx;
  margin-bottom: 40rpx;
}
.login-phone-getcode {
  color: #3F51B5;
  text-align: center;
  width: 230rpx;
  /* min-width: 130upx;
	max-width: 160upx; */
  /* font-size: 30upx; */
}
.login-code {
  border-bottom: 2rpx #dedede solid;
}
.btn-login {
  margin-top: 100rpx;
  border-radius: 50rpx;
  font-size: 16px;
  color: #fff;
  background: -webkit-linear-gradient(left, #88a1f9, #9ac6ff);
  background: linear-gradient(to right, #88a1f9, #9ac6ff);
}
.btn-login-active {
  background: -webkit-linear-gradient(left, #365fff, #36bbff);
  background: linear-gradient(to right, #365fff, #36bbff);
}
.btn-login-hover {
  background: -webkit-linear-gradient(left, #365fdd, #36bbfa);
  background: linear-gradient(to right, #365fdd, #36bbfa);
}
.login-username {
  margin-top: 56rpx;
  margin-bottom: 40rpx;
  border-bottom: 2rpx #dedede solid;
}
.login-password {
  border-bottom: 2rpx #dedede solid;
}
button[disabled] {
  color: #fff;
}
.login-mode {
  text-align: center;
  margin-top: 32rpx;
}
.otherLoginWays {
  top: 50rpx;
  width: 80%;
  position: relative;
  bottom: 8.2%;
  left: 10%;
  right: 10%;
  text-align: center;
}
.otherLoginWays .otherWayTextWrapper {
  line-height: 0;
}
.otherLoginWays .otherWayTextWrapper:before, .otherLoginWays .otherWayTextWrapper:after {
  position: absolute;
  background: #ddd;
  content: "";
  height: 1px;
  width: 30%;
}
.otherLoginWays .otherWayTextWrapper:before {
  left: 0;
}
.otherLoginWays .otherWayTextWrapper:after {
  right: 0;
}
.otherLoginWays .otherWayTextWrapper .otherWayText {
  font-size: .8rem;
  color: #bbbbbb;
}
.otherLoginWays .icons {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  margin-top: 1.4em;
}
.otherLoginWays .icons ._img {
  width: 2rem;
  height: 2em;
}
.protocol {
  position: fixed;
  bottom: 4%;
  left: 2%;
  right: 2%;
  font-size: .72em;
  margin-top: 8%;
  color: #333333;
}
.protocol ._span {
  color: #2986de;
}

